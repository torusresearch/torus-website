{
  "name": "app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production npm run build-embed:production && npm run build-embed:copy-main && npm run pre-build && vue-cli-service build",
    "build:staging": "cross-env NODE_ENV=production npm run build-embed:staging && npm run build-embed:copy-main && npm run pre-build && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-embed:copy-main": "cp ./public/embed.min.js ./public/embed-main.min.js",
    "build-embed:dev": "cross-env NODE_ENV=development node ./scripts/createEmbed.js && npm run test:build-embed",
    "build-embed:production": "cross-env NODE_ENV=production node ./scripts/createEmbed.js && npm run test:build-embed",
    "build-embed:staging": "cross-env NODE_ENV=staging node ./scripts/createEmbed.js && npm run test:build-embed",
    "build-embed:report": "npm run build-embed:production && discify ./public/embed.min.js > disc.html",
    "build:dev": "npm run build-embed:dev && npm run build-embed:copy-main && npm run pre-build && vue-cli-service build --mode development",
    "build:report": "vue-cli-service build --report",
    "dev": "cross-env NODE_ENV=development node server.js",
    "lint:es": "eslint ./src/views/* --fix",
    "pre-build": "node ./scripts/calculateIntegrity.js",
    "production": "cross-env PORT=443 NODE_ENV=production node server.js",
    "temp": "npm run build-embed:dev && npm run build:dev",
    "test:codecov-upload": "if [ $CODECOV_TOKEN ]; then nyc report --reporter=text-lcov > coverage.lcov && codecov; fi",
    "test:coverage-codecov": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:codecov-upload",
    "test:coverage-coveralls": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:coveralls-upload",
    "test:coveralls-upload": "if [ $COVERALLS_REPO_TOKEN ]; then nyc report --reporter=text-lcov | coveralls; fi",
    "test:e2e": "node ./test/e2e/puppeteer.js",
    "test:e2e:headful": "node ./test/e2e/puppeteer.js false",
    "test:unit": "mocha --exit -r esm -r jsdom-global/register -r isomorphic-fetch ./test/unit/controllers/*.js ./test/unit/controllers/transactions/*.js",
    "test:build-embed": "mocha --exit ./test/compile/Web3Test.js"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "assert": "^1.4.1",
    "backoff": "^2.5.0",
    "broadcast-channel": "^2.1.12",
    "copy-to-clipboard": "^3.1.0",
    "core-js": "2.6.5",
    "debounce": "^1.2.0",
    "dnode": "^1.2.2",
    "dotenv": "^8.0.0",
    "eth-block-tracker": "^4.3.0",
    "eth-json-rpc-filters": "^4.0.2",
    "eth-json-rpc-middleware": "^4.1.2",
    "eth-query": "^2.1.2",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-vm": "3.0.0",
    "ethereumjs-wallet": "^0.6.3",
    "ethjs-query": "^0.3.8",
    "fast-json-patch": "^2.1.0",
    "human-standard-token-abi": "^2.0.0",
    "json-rpc-engine": "^5.0.0",
    "json-rpc-middleware-stream": "^2.1.1",
    "jsonschema": "^1.2.4",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^4.0.2",
    "percentile": "^1.2.1",
    "pify": "^4.0.1",
    "post-message-stream": "^3.0.0",
    "psl": "^1.1.31",
    "random-id": "^1.0.3",
    "readable-stream": "^3.3.0",
    "swappable-obj-proxy": "^1.1.0",
    "through2": "^3.0.1",
    "ua-parser-js": "^0.7.19",
    "vue": "^2.6.10",
    "vue-knob-control": "^1.3.1",
    "vue-router": "^3.0.6",
    "vue-script2": "^2.0.3",
    "vuetify": "^1.5.13",
    "vuex": "^3.1.0",
    "vuex-persist": "^2.0.0",
    "web3": "1.0.0-beta.52",
    "web3-provider-engine": "^15.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "browserify": "^16.2.3",
    "codecov": "^3.3.0",
    "cross-env": "^5.2.0",
    "envify": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "3.2.20",
    "eth-phishing-detect": "^1.1.13",
    "ganache-core": "^2.5.5",
    "install": "^0.12.2",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "15.0.0",
    "jsdom-global": "3.0.2",
    "luxon": "^1.12.1",
    "mocha": "^6.1.4",
    "mocha-eslint": "^5.0.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-sinon": "^2.1.0",
    "mock-browser": "^0.92.14",
    "nock": "^10.0.6",
    "node-sass": "^4.11.0",
    "nyc": "^14.0.0",
    "prerender-spa-plugin": "^3.4.0",
    "puppeteer": "^1.14.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "sri-toolbox": "^0.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglifyify": "^5.0.1",
    "vue-cli-plugin-prerender-spa": "^1.1.5",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "mocha": {
    "timeout": 0
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "10.x"
  }
}

{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "browserify ./src/widget/embed.js -o public/embed.min.js && vue-cli-service serve",
    "build": "vue-cli-service build",
    "buildEmbed": "browserify ./src/widget/embed.js -o public/embed.min.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "ajv": "^6.6.2",
    "backoff": "^2.5.0",
    "browserify": "^16.2.3",
    "eth-json-rpc-filters": "^3.0.1",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-vm": "2.4.0",
    "json-rpc-engine": "^4.0.0",
    "json-rpc-middleware-stream": "^2.1.0",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^3.0.2",
    "post-message-stream": "^3.0.0",
    "vue": "^2.5.21",
    "vue-router": "^3.0.1",
    "vue-script2": "^2.0.3",
    "vuex": "^3.0.1",
    "vuex-persist": "^2.0.0",
    "web3": "^1.0.0-beta.37",
    "web3-provider-engine": "^14.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "overrides": [
      {
        "files": [
          "*/vendor/*"
        ],
        "excludedFiles": "*"
      }
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}

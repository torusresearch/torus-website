{
  "name": "app",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production npm run build-embed:production && npm run build-embed:copy-main && npm run pre-build && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-embed:copy-main": "cp ./public/embed.min.js ./public/embed-main.min.js",
    "build-embed:dev": "$(npm bin)/browserify --full-paths -t [ $(npm root)/browserify-replace --replace '{ \"from\": \"<BROWSERIFY_REPLACE_MODE>\", \"to\": \"development\" }' --replace '{ \"from\": \"<BROWSERIFY_REPLACE_VENDOR_WEB3>\", \"to\": \"web3.js\" }' ] --noparse=$(pwd)/inpage/vendor/web3.js --noparse=$(pwd)/inpage/vendor/web3.min.js ./inpage/embed.js -o public/embed.min.js",
    "build-embed:production": "$(npm bin)/browserify --full-paths -t [ $(npm root)/browserify-replace --replace '{ \"from\": \"<BROWSERIFY_REPLACE_MODE>\", \"to\": \"production\" }' --replace '{ \"from\": \"<BROWSERIFY_REPLACE_VENDOR_WEB3>\", \"to\": \"web3.min.js\" }' ] --noparse=$(pwd)/inpage/vendor/web3.js --noparse=$(pwd)/inpage/vendor/web3.min.js ./inpage/embed.js -o public/embed-temp.min.js && $(npm bin)/browserify --full-paths -g uglifyify ./public/embed-temp.min.js > ./public/embed.min.js",
    "build-embed:report": "npm run build-embed:production && discify ./public/embed.min.js > disc.html",
    "build:dev": "npm run build-embed:dev && npm run build-embed:copy-main && npm run pre-build && vue-cli-service build --mode development",
    "build:report": "vue-cli-service build --report",
    "dev": "cross-env NODE_ENV=development node server.js",
    "lint:es": "eslint ./src/views/* --fix",
    "pre-build": "node ./scripts/calculateIntegrity.js",
    "production": "cross-env PORT=443 NODE_ENV=production node server.js",
    "temp": "npm run build-embed:dev && npm run build:dev",
    "test:codecov-upload": "if [ $CODECOV_TOKEN ]; then nyc report --reporter=text-lcov > coverage.lcov && codecov; fi",
    "test:coverage-codecov": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:codecov-upload",
    "test:coverage-coveralls": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:coveralls-upload",
    "test:coveralls-upload": "if [ $COVERALLS_REPO_TOKEN ]; then nyc report --reporter=text-lcov | coveralls; fi",
    "test:e2e": "npm run build-embed:dev && npm run build:dev && (npm run dev & pid1=\"$!\"; node ./test/e2e/puppeteer.js && kill $pid1)",
    "test:e2e:headful": "npm run build-embed:dev && npm run build:dev && (npm run dev & pid1=\"$!\"; node ./test/e2e/puppeteer.js false && kill $pid1)",
    "test:unit": "mocha --exit -r esm -r jsdom-global/register ./test/unit/controllers/*.js ./test/unit/controllers/transactions/*.js"
  },
  "dependencies": {
    "ajv": "^6.10.0",
    "assert": "^1.4.1",
    "backoff": "^2.5.0",
    "broadcast-channel": "^2.1.10",
    "browserify": "^16.2.3",
    "browserify-replace": "^1.0.0",
    "debounce": "^1.2.0",
    "dnode": "^1.2.2",
    "dotenv": "^7.0.0",
    "eth-block-tracker": "^4.3.0",
    "eth-json-rpc-filters": "^4.0.1",
    "eth-json-rpc-middleware": "^4.1.2",
    "eth-query": "^2.1.2",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-vm": "2.6.0",
    "ethereumjs-wallet": "^0.6.3",
    "fast-json-patch": "^2.0.7",
    "human-standard-token-abi": "^2.0.0",
    "json-rpc-engine": "^5.0.0",
    "json-rpc-middleware-stream": "^2.1.1",
    "jsonschema": "^1.2.4",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^4.0.2",
    "percentile": "^1.2.1",
    "pify": "^4.0.1",
    "post-message-stream": "^3.0.0",
    "random-id": "^1.0.3",
    "readable-stream": "^3.2.0",
    "swappable-obj-proxy": "^1.1.0",
    "through2": "^3.0.1",
    "ua-parser-js": "^0.7.19",
    "uglifyify": "^5.0.1",
    "vue": "^2.6.10",
    "vue-router": "^3.0.2",
    "vue-script2": "^2.0.3",
    "vuetify": "^1.5.7",
    "vuex": "^3.1.0",
    "vuex-persist": "^2.0.0",
    "web3": "1.0.0-beta.37",
    "web3-provider-engine": "^15.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "@vue/cli-service": "^3.5.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "^3.2.20",
    "eth-phishing-detect": "^1.1.13",
    "ganache-core": "^2.5.3",
    "jsdom": "14.0.0",
    "jsdom-global": "3.0.2",
    "luxon": "^1.12.0",
    "mocha": "^6.0.2",
    "mocha-eslint": "^5.0.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-sinon": "^2.1.0",
    "nock": "^10.0.6",
    "nyc": "^13.3.0",
    "prerender-spa-plugin": "^3.4.0",
    "puppeteer": "^1.13.0",
    "sinon": "^7.3.1",
    "sri-toolbox": "^0.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-prerender-spa": "^1.1.5",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "10.x"
  }
}

{
  "name": "app",
  "version": "1.4.4",
  "private": true,
  "scripts": {
    "serve": "npm run locale && vue-cli-service serve",
    "build": "cross-env VUE_APP_TORUS_BUILD_ENV=production npm run pre-build && vue-cli-service build && npm run post-build",
    "lint": "vue-cli-service lint",
    "build:dev": "cross-env VUE_APP_TORUS_BUILD_ENV=development npm run pre-build && vue-cli-service build --mode development",
    "build:staging": "cross-env VUE_APP_TORUS_BUILD_ENV=staging npm run pre-build && vue-cli-service build --mode staging && npm run post-build",
    "build:testing": "cross-env VUE_APP_TORUS_BUILD_ENV=testing npm run pre-build && vue-cli-service build --mode testing",
    "build:lrc": "cross-env VUE_APP_TORUS_BUILD_ENV=lrc npm run pre-build && vue-cli-service build --mode lrc",
    "dev": "cross-env NODE_ENV=development VUE_APP_TORUS_BUILD_ENV=development node server.js",
    "locale": "node ./scripts/importLocales.js",
    "lint:js": "eslint --fix 'src/**/*.js'",
    "lint:vue": "eslint --fix 'src/**/*.vue'",
    "post-build": ". ./scripts/buildErrorPage.sh",
    "pre-build": "node ./scripts/replaceRedirectHTML.js && npm run locale",
    "production": "cross-env PORT=443 NODE_ENV=production node server.js",
    "test:unit": "npm run locale && mocha ./test/unit/controllers/transactions/*.js ./test/unit/controllers/*.js",
    "test:e2e": "mocha --timeout=60000 ./test/e2e",
    "test:codecov-upload": "if [ $CODECOV_TOKEN ]; then nyc report --reporter=text-lcov > coverage.lcov && codecov; fi",
    "test:coverage-codecov": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:codecov-upload",
    "test:e2e:headful": "node ./test/e2e/puppeteer.js false",
    "test:e2e:mobile": "env MOBILE_ENV=true mocha --timeout=60000 ./test/e2e"
  },
  "dependencies": {
    "@chaitanyapotti/random-id": "^1.0.3",
    "@toruslabs/fetch-node-details": "^2.2.2",
    "@toruslabs/torus.js": "^2.1.3",
    "assert": "^2.0.0",
    "await-semaphore": "^0.1.3",
    "bignumber.js": "^9.0.0",
    "broadcast-channel": "^3.1.0",
    "clone": "^2.1.2",
    "copy-to-clipboard": "^3.3.1",
    "core-js": "^3.6.5",
    "create-hash": "^1.2.0",
    "deepmerge": "^4.2.2",
    "eth-block-tracker": "^4.4.3",
    "eth-contract-metadata": "^1.13.0",
    "eth-json-rpc-errors": "^2.0.2",
    "eth-json-rpc-filters": "^4.1.1",
    "eth-json-rpc-infura": "^4.0.2",
    "eth-json-rpc-middleware": "^4.4.1",
    "eth-query": "^2.1.2",
    "eth-sig-util": "^2.5.3",
    "ethereumjs-common": "^1.5.1",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-util": "^7.0.0",
    "ethereumjs-wallet": "^0.6.3",
    "ethjs-query": "^0.3.8",
    "fast-json-patch": "^2.2.1",
    "human-standard-collectible-abi": "^1.0.2",
    "human-standard-token-abi": "^2.0.0",
    "json-rpc-engine": "^5.1.8",
    "json-rpc-middleware-stream": "^2.1.1",
    "jsonschema": "^1.2.6",
    "jwt-decode": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "loglevel": "^1.6.8",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^4.0.3",
    "percentile": "^1.3.0",
    "pify": "^5.0.0",
    "post-message-stream": "^3.0.0",
    "promise-to-callback": "^1.0.0",
    "pump": "^3.0.0",
    "readable-stream": "^3.6.0",
    "register-service-worker": "^1.7.1",
    "safe-event-emitter": "^1.0.1",
    "single-call-balance-checker-abi": "^1.0.0",
    "swappable-obj-proxy": "^1.1.0",
    "vue": "^2.6.11",
    "vue-content-loader": "^0.2.3",
    "vue-gtm": "^2.2.0",
    "vue-json-pretty": "^1.6.3",
    "vue-qr": "^2.2.1",
    "vue-qrcode-reader": "^2.3.0",
    "vue-router": "^3.1.6",
    "vue-spinner": "^1.0.3",
    "vuetify": "^2.2.28",
    "vuex": "^3.4.0",
    "vuex-persist": "^2.2.0",
    "web3": "^1.2.7",
    "web3-utils": "^1.2.7"
  },
  "devDependencies": {
    "@babel/register": "^7.9.0",
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-pwa": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "envify": "^4.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-problems": "^4.0.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^5.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-only-error": "^1.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-unicorn": "^19.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "eslint-plugin-vue-scoped-css": "^0.5.0",
    "esm": "^3.2.25",
    "eth-phishing-detect": "^1.1.13",
    "express": "^4.17.1",
    "fibers": "^5.0.0",
    "ganache-core": "^2.10.2",
    "husky": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^16.2.2",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.2.2",
    "luxon": "^1.24.1",
    "mocha": "^7.1.2",
    "mocha-eslint": "^6.0.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-sinon": "^2.1.2",
    "nock": "12.0.3",
    "node-fetch": "^2.6.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "puppeteer": "^3.0.4",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sinon": "^9.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "terser-webpack-plugin": "2.3.6",
    "through2": "^3.0.1",
    "uglifyify": "^5.0.2",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3",
    "worker-loader": "^2.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js --",
      "prettier --write 'src/**/*.js'"
    ],
    "*.vue": [
      "npm run lint:vue --",
      "prettier --write 'src/**/*.vue'"
    ],
    "*.{json,css,yaml,yml}": [
      "prettier --write '**/*.json'",
      "prettier --write '**/*.css'",
      "prettier --write '**/*.yaml'"
    ]
  },
  "mocha": {
    "timeout": 0
  }
}

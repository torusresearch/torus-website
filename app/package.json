{
  "name": "app",
  "version": "0.1.1",
  "buildVersion": "v2",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production TORUS_BUILD_ENV=production npm run build-embed:production && npm run pre-build && cross-env TORUS_BUILD_ENV=production vue-cli-service build",
    "build:staging": "cross-env NODE_ENV=production TORUS_BUILD_ENV=staging npm run build-embed:staging && npm run pre-build && cross-env TORUS_BUILD_ENV=staging vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-embed:dev": "cross-env TORUS_BUILD_ENV=development node ./scripts/createEmbed.js",
    "build-embed:testing": "cross-env TORUS_BUILD_ENV=testing node ./scripts/createEmbed.js",
    "build-embed:production": "cross-env TORUS_BUILD_ENV=production NODE_ENV=production node ./scripts/createEmbed.js",
    "build-embed:staging": "cross-env TORUS_BUILD_ENV=staging node ./scripts/createEmbed.js && npm run test:build-embed",
    "build-embed:report": "npm run build-embed:production && discify ./public/embed.min.js > disc.html",
    "build:dev": "npm run build-embed:dev && npm run pre-build && vue-cli-service build --mode development",
    "build:testing": "npm run build-embed:testing && npm run pre-build && vue-cli-service build --mode development",
    "build:report": "vue-cli-service build --report",
    "dev": "cross-env NODE_ENV=development TORUS_BUILD_ENV=development node server.js",
    "lint:es": "eslint ./src/views/* --fix",
    "pre-build": ". ./scripts/calculateIntegrity.sh",
    "production": "cross-env PORT=443 NODE_ENV=production node server.js",
    "temp": "npm run build-embed:dev && npm run build:dev",
    "test:codecov-upload": "if [ $CODECOV_TOKEN ]; then nyc report --reporter=text-lcov > coverage.lcov && codecov; fi",
    "test:coverage-codecov": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:codecov-upload",
    "test:coverage-coveralls": "nyc --reporter=text --reporter=html npm run test:unit && npm run test:coveralls-upload",
    "test:coveralls-upload": "if [ $COVERALLS_REPO_TOKEN ]; then nyc report --reporter=text-lcov | coveralls; fi",
    "test:e2e": "node ./test/e2e/puppeteer.js",
    "test:e2e:headful": "node ./test/e2e/puppeteer.js false",
    "test:unit": "mocha --exit -r esm -r jsdom-global/register -r isomorphic-fetch ./test/unit/controllers/*.js ./test/unit/controllers/transactions/*.js",
    "test:build-embed": "mocha --exit ./test/compile/Web3Test.js",
    "update:buildVersion": "node ./scripts/updatebuildVersion.js"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "backoff": "^2.5.0",
    "broadcast-channel": "^2.1.12",
    "copy-to-clipboard": "^3.2.0",
    "core-js": "2.6.5",
    "debounce": "^1.2.0",
    "eth-block-tracker": "^4.4.1",
    "eth-contract-metadata": "^1.9.0",
    "eth-json-rpc-filters": "^4.0.3",
    "eth-json-rpc-middleware": "^4.1.3",
    "eth-query": "^2.1.2",
    "eth-sig-util": "^2.2.0",
    "ethereumjs-tx": "1.3.7",
    "ethereumjs-util": "^6.1.0",
    "ethereumjs-wallet": "^0.6.3",
    "ethjs-query": "^0.3.8",
    "fast-json-patch": "^2.1.0",
    "human-standard-token-abi": "^2.0.0",
    "json-rpc-engine": "^5.0.0",
    "json-rpc-middleware-stream": "^2.1.1",
    "jsonschema": "^1.2.4",
    "obj-multiplex": "^1.0.0",
    "obs-store": "^4.0.2",
    "percentile": "^1.2.1",
    "pify": "^4.0.1",
    "post-message-stream": "^3.0.0",
    "psl": "^1.1.32",
    "random-id": "^1.0.3",
    "readable-stream": "^3.4.0",
    "single-call-balance-checker-abi": "^1.0.0",
    "swappable-obj-proxy": "^1.1.0",
    "ua-parser-js": "^0.7.20",
    "vue": "^2.6.10",
    "vue-knob-control": "^1.4.1",
    "vue-router": "^3.0.6",
    "vuetify": "^1.5.14",
    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1",
    "web3": "1.0.0-beta.37",
    "web3-provider-engine": "^15.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "@toruslabs/torus-embed": "^0.0.14",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "browserify": "^16.2.3",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "envify": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.9.1",
    "eslint-plugin-html": "^5.0.5",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "esm": "3.2.25",
    "eth-phishing-detect": "^1.1.13",
    "ganache-core": "^2.5.5",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "15.1.1",
    "jsdom-global": "3.0.2",
    "luxon": "^1.16.0",
    "mocha": "^6.1.4",
    "mocha-eslint": "^5.0.0",
    "mocha-jsdom": "^2.0.0",
    "mocha-sinon": "^2.1.0",
    "mock-browser": "^0.92.14",
    "nock": "^10.0.6",
    "node-sass": "^4.12.0",
    "nyc": "^14.1.1",
    "prerender-spa-plugin": "^3.4.0",
    "puppeteer": "^1.17.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.3.2",
    "sri-toolbox": "^0.2.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "through2": "^3.0.1",
    "uglifyify": "^5.0.1",
    "vue-cli-plugin-prerender-spa": "^1.1.5",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.2.2"
  },
  "mocha": {
    "timeout": 0
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "engines": {
    "node": "10.x"
  }
}